    <connection>gradilisteOp</connection>
    <sql>SELECT 
  ZM1.IdZM AS IdZM
, Z1.JMBG AS JMBG
, (SELECT IdZps 
FROM gradilisteDw.dbo.Zaposlenost 
WHERE gradilisteDw.dbo.Zaposlenost.Zaposljavanje = 'pocetak' ) AS IdZps
, (SELECT IdLok
FROM gradilisteDw.dbo.Lokacija
WHERE 
gradilisteDw.dbo.Lokacija.Ulica = G1.Ulica AND
gradilisteDw.dbo.Lokacija.Broj = G1.Broj AND
gradilisteDw.dbo.Lokacija.Opstina = Og1.Opstina AND
gradilisteDw.dbo.Lokacija.Grad = Og1.Grad AND
gradilisteDw.dbo.Lokacija.Drzava = Dg1.Drzava
) AS IdLokMag
, (SELECT IdLok
FROM gradilisteDw.dbo.Lokacija
WHERE
gradilisteDw.dbo.Lokacija.Ulica = Z1.Ulica AND
gradilisteDw.dbo.Lokacija.Broj = Z1.Broj AND
gradilisteDw.dbo.Lokacija.Opstina = Oz1.Opstina AND
gradilisteDw.dbo.Lokacija.Grad = Oz1.Grad AND
gradilisteDw.dbo.Lokacija.Drzava = Dz1.Drzava
) AS IdLokZap
, (SELECT IdVre
FROM gradilisteDw.dbo.Vreme
WHERE
gradilisteDw.dbo.Vreme.DatumVreme = PocetakRada.DatumVreme
) AS IdVre,
1 AS Doprinos
FROM Zaposleni Z1, PocetakRada, ZonaMagacina ZM1, Magacin M1, Gradiliste G1, Opstina Oz1, Drzava Dz1, Opstina Og1, Drzava Dg1
WHERE 
Z1.JMBG = PocetakRada.JMBG AND
Z1.IdO = Oz1.IdO AND
Oz1.IdD = Dz1.IdD AND
PocetakRada.IdZM = ZM1.IdZM AND
ZM1.IdMag = M1.IdMag AND
M1.IdG = G1.IdG AND
G1.IdO = Og1.IdO AND
Og1.IdD = Dg1.IdD 
AND 
(PocetakRada.DatumVreme >
(SELECT Punjenje.PoslednjePunjenje
FROM gradilisteDw.dbo.Punjenje))

UNION

SELECT 
  ZM2.IdZM AS IdZM
, Z2.JMBG AS JMBG
, (SELECT IdZps 
FROM gradilisteDw.dbo.Zaposlenost 
WHERE gradilisteDw.dbo.Zaposlenost.Zaposljavanje = 'kraj' ) AS IdZps
, (SELECT IdLok
FROM gradilisteDw.dbo.Lokacija
WHERE 
gradilisteDw.dbo.Lokacija.Ulica = G2.Ulica AND
gradilisteDw.dbo.Lokacija.Broj = G2.Broj AND
gradilisteDw.dbo.Lokacija.Opstina = Og2.Opstina AND
gradilisteDw.dbo.Lokacija.Grad = Og2.Grad AND
gradilisteDw.dbo.Lokacija.Drzava = Dg2.Drzava
) AS IdLokMag
, (SELECT IdLok
FROM gradilisteDw.dbo.Lokacija
WHERE
gradilisteDw.dbo.Lokacija.Ulica = Z2.Ulica AND
gradilisteDw.dbo.Lokacija.Broj = Z2.Broj AND
gradilisteDw.dbo.Lokacija.Opstina = Oz2.Opstina AND
gradilisteDw.dbo.Lokacija.Grad = Oz2.Grad AND
gradilisteDw.dbo.Lokacija.Drzava = Dz2.Drzava
) AS IdLokZap
, (SELECT IdVre
FROM gradilisteDw.dbo.Vreme
WHERE
gradilisteDw.dbo.Vreme.DatumVreme = KrajRada.DatumVreme
) AS IdVre,
-1 AS Doprinos
FROM Zaposleni Z2, KrajRada, ZonaMagacina ZM2, Magacin M2, Gradiliste G2, Opstina Oz2, Drzava Dz2, Opstina Og2, Drzava Dg2
WHERE 
Z2.JMBG = KrajRada.JMBG AND
Z2.IdO = Oz2.IdO AND
Oz2.IdD = Dz2.IdD AND
KrajRada.IdZM = ZM2.IdZM AND
ZM2.IdMag = M2.IdMag AND
M2.IdG = G2.IdG AND
G2.IdO = Og2.IdO AND
Og2.IdD = Dg2.IdD 
AND 
(KrajRada.DatumVreme >
(SELECT Punjenje.PoslednjePunjenje FROM gradilisteDw.dbo.Punjenje))


</sql>
