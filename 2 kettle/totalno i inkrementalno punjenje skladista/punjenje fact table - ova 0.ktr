  <step>
    <name>Operativna baza - Nabavka 2</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>gradilisteOp</connection>
    <sql>select 
M.IdMat as IdMat,
ZM.IdZM as IdZM,
Lok.IdLok as IdLokGra,
DAN.IdDan as IdDan,

(select sum(Nab.Kolicina)
 from Nabavka Nab
 where Nab.IdMat=M.IdMat
 and Nab.IdZM=ZM.IdZM
 and CONVERT(date, Nab.DatumVreme)&lt;=DAN.Datum)

-(select case 
  when sum(Pot.Kolicina) IS NOT NULL

     then sum(Pot.Kolicina)

     else 0
 end as Broj
 from Potrosnja Pot, Nabavka Nab
 where Pot.IdN=Nab.IdN
 and Nab.IdMat=M.IdMat
 and Nab.IdZM=ZM.IdZM
 and CONVERT(date, Pot.PoslednjaPromena)&lt;=DAN.Datum)
 
as Kolicina


from ZonaMagacina ZM, Materijal M, gradilisteDw.dbo.Lokacija Lok, Nabavka N, Magacin Mag, Gradiliste G, gradilisteDw.dbo.Dan DAN


where ZM.IdZM = N.IdZM
and N.IdMat = M.IdMat
and DAN.Datum = CONVERT(date, N.DatumVreme)
and Mag.IdMag = ZM.IdMag
and Mag.IdG = G.IdG
and Lok.Broj = G.Broj
and Lok.Ulica = G.Ulica
and Lok.Opstina = (SELECT O.Opstina FROM Opstina O WHERE  G.IdO = O.IdO )
and Lok.Grad = (SELECT O.Grad FROM Opstina O WHERE  G.IdO = O.IdO )
and Lok.Drzava = (SELECT D.Drzava FROM Drzava D, Opstina O WHERE O.IdO = G.IdO AND O.IdD = D.IdD )
and (select sum(Nab.Kolicina)
 from Nabavka Nab
 where Nab.IdMat=M.IdMat
 and Nab.IdZM=ZM.IdZM
 and CONVERT(date, Nab.DatumVreme)&lt;=DAN.Datum) IS NOT NULL
and NOT EXISTS(select *
      from gradilisteDw.dbo.StanjeMaterijali SM
               where SM.IdMat=M.IdMat
      and SM.IdDan=DAN.IdDan
      and SM.IdZM=ZM.IdZM)
and N.DatumVreme>(select gradilisteDw.dbo.Punjenje.PoslednjePunjenje
      from gradilisteDw.dbo.Punjenje)
group by M.IdMat, ZM.IdZM, Lok.IdLok, DAN.Datum, Dan.IdDan

</sql>
