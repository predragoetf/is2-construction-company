  <step>
    <name>Operativna baza - Nabavka</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>gradilisteOp</connection>
    <sql>SELECT 
  Distributer.PIB AS PIB
, ZonaMagacina.IdZM AS IdZM
, Nabavka.Kolicina AS Kolicina
, Nabavka.Kolicina * Nabavka.JedinicnaCena AS Iznos
, ( SELECT IdLok 
FROM gradilisteDw.dbo.Lokacija 
WHERE 
gradilisteDw.dbo.Lokacija.Ulica = Gradiliste.Ulica AND
gradilisteDw.dbo.Lokacija.Broj = Gradiliste.Broj AND
gradilisteDw.dbo.Lokacija.Opstina = Og.Opstina AND
gradilisteDw.dbo.Lokacija.Grad = Og.Grad AND
gradilisteDw.dbo.Lokacija.Drzava = Dg.Drzava
) AS IdLokGra
, ( SELECT IdLok 
FROM gradilisteDw.dbo.Lokacija 
WHERE 
gradilisteDw.dbo.Lokacija.Ulica = Distributer.Ulica AND
gradilisteDw.dbo.Lokacija.Broj = Distributer.Broj AND
gradilisteDw.dbo.Lokacija.Opstina = Od.Opstina AND
gradilisteDw.dbo.Lokacija.Grad = Od.Grad AND
gradilisteDw.dbo.Lokacija.Drzava = Dd.Drzava
) AS IdLokDist
, ( SELECT IdVre
FROM gradilisteDw.dbo.Vreme
WHERE 
gradilisteDw.dbo.Vreme.DatumVreme = Nabavka.DatumVreme
) AS IdVre
, Nabavka.IdN AS IdNab
FROM Nabavka, Distributer, ZonaMagacina, Magacin,  Gradiliste, Opstina Od, Opstina Og, Drzava Dd, Drzava Dg
WHERE
Nabavka.IdZM = ZonaMagacina.IdZM AND
Nabavka.PIB = Distributer.PIB AND
ZonaMagacina.IdMag = Magacin.IdMag AND
Magacin.IdG = Gradiliste.IdG AND
Gradiliste.IdO = Og.IdO AND
Og.IdD = Dg.IdD AND
Distributer.IdO = Od.IdO AND
Od.IdD = Dd.IdD
AND 
(Nabavka.DatumVreme > (SELECT Punjenje.PoslednjePunjenje
from gradilisteDw.dbo.Punjenje))
</sql>
